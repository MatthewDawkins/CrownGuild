<%- include("header") -%>

<!-- Background Image -->
<div class="">
  <img class="background-img" src="images/arathibasin.jpg" alt="">
</div>

<section id="pvp-carousel">

  <h1 class="pvp-slogan">Crown is home to some of the best PvPers in the world.</h1>

    <div class="row">

      <!-- Youtube video carousel -->
      <div class="col-lg-6">
        <h3>Tourament Champions</h3>
        <div id="carouselExampleIndicators" class="carousel slide" data-interval="false">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <iframe width="900" height="300" src="https://www.youtube.com/embed/w7A3HKI-XrY" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
            <div class="carousel-item">
              <iframe width="900" height="300" src="https://www.youtube.com/embed/YVHAFITM_-w" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
            <div class="carousel-item">
              <iframe width="900" height="300" src="https://www.youtube.com/embed/6gB2quowFqo" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            </div>
          </div>
          <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
          </a>
        </div>
      </div>

      <!-- Rank one images carousel -->
      <div class="col-lg-6">
        <h3>Rank Ones</h3>
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img class="d-block w-100 achievement" src="/images/prideful.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/wild.png" alt="Second slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/warmongering.png" alt="Third slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/vindictive.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/fearless.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/cruel.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/fierce.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/dominant.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/demonic.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/dread.png" alt="First slide">
            </div>
            <div class="carousel-item">
              <img class="d-block w-100 achievement" src="/images/sinister.png" alt="First slide">
            </div>
          </div>
          <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>

    </div>

    <!-- Crown credits -->
    <p class="credit">Crown Â© <%= year %></p>

</section>

<%- include("footer") -%>
